// Please be aware that this script is loaded directly in HEAD.
// Do not use ES6 Syntax or Code that requires polyfilling in IE
// noinspection ES6ConvertVarToLetConst

(function () {
  "use strict";

  var searchExports = {
    locale: '<%= I18n.locale[0..1] %>',
    translations: {
      search: <%== js_translations_export(I18n.t('search')) %>,
    },
    algolia: {
      applicationId: '<%= ENV['ALGOLIA_APPLICATION_ID'] %>',
      apiKey: '<%= ENV['ALGOLIA_FRONTEND_API_KEY'] %>',
    }
  };

  if (!('MyZivi' in window)) {
    window.MyZivi = searchExports;
  } else {
    $.extend(true, window.MyZivi, searchExports);
  }
})();
