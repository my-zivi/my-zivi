(function () {
  "use strict";

  var servicesOverviewExports = {
    locale: '<%= I18n.locale[0..1] %>',
    translations: {
      loading: '<%= I18n.t('loading') %>',
      servicesOverview: {
        title: '<%= I18n.t('organizations.services.index.title') %>',
        errors: {
          cannotLoad: '<%= I18n.t('organizations.services.index.errors.cannot_load') %>'
        }
      },
      error: '<%= I18n.t('error') %>'
    },
    paths: {
      servicesOverview: '<%= organizations_services_path(format: :json) %>'
    }
  };

  if (!('MyZivi' in window)) {
    window.MyZivi = servicesOverviewExports;
  } else {
    $.extend(true, window.MyZivi, servicesOverviewExports);
  }
})();
