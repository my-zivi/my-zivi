  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.6",
    "inferno": "^5.0.4",
    "inferno-router": "^5.0.4",
    "inferno-scripts": "6.3.0",
    "izitoast": "^1.1.5",
    "jwt-decode": "^2.2.0",
    "md-colors": "^1.0.0",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "raven-js": "^3.22.1",
    "survey-jquery": "^0.12.21"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "inferno-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "inferno-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "inferno-scripts test --env=jsdom",
    "eject": "inferno-scripts eject",
    "format": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "inferno-devtools": "^5.0.4",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1"
  }
}
