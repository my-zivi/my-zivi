{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "setasign/fpdi-fpdf": "1.6.2",
        "globalcitizen/php-iban": "^2.6",
        "sentry/sentry": "^1.8.3",
        "sentry/sentry-laravel": "^0.8.0",
        "doctrine/dbal": "^2.7",
        "dingo/api": "v2.0.0-alpha2",
        "tymon/jwt-auth": "1.0.0-rc.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "flipbox/lumen-generator": "5.5.*",
        "squizlabs/php_codesniffer": "~3.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "format": "phpcbf --standard=psr2 --ignore=vendor ."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
